<UserControl x:Class="MajiaobaGeoSystem.Pages.DisasterManage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             xmlns:local="clr-namespace:MajiaobaGeoSystem.Pages"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab SelectedSource="/Content/DisasterManage/DisasterList.xaml" Layout="List">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="灾害点/隐患点列表"   Source="/Content/DisasterManage/DisasterList.xaml" />
                <mui:Link DisplayName="新增"               Source="/Content/DisasterManage/DisasterAdd.xaml"/>
                <mui:Link DisplayName="详细信息"           Source="/Content/DisasterManage/DisasterDetail.xaml" />
                <mui:Link DisplayName="修改"               Source="/Content/DisasterManage/DisasterEdit.xaml"/>
                <mui:Link DisplayName="删除"               Source="/Content/DisasterManage/DisasterDeleteConfirm.xaml"/>
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
